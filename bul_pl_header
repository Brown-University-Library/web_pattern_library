<script type="text/javascript">
// When the user scrolls down 50px from the top of the document, resize the header's font size
window.onscroll = function() {scrollFunction()};

function scrollFunction() {
  if (document.body.scrollTop > 42 || document.documentElement.scrollTop > 42) {
    document.getElementById("bul_pl_universal_header").style.height = "48";
    document.getElementById("bul_pl_universal_header").style.padding = "0px 0px 15px 25px";
    document.getElementById("bul_pl_brown_shield_svg").style.height = "45";
    document.getElementById("bul_pl_header_links").style.margin = "3ex 50px 0 0";
    document.getElementById("bul_pl_logo_text").style.margin = "25px 0px 0px 0px";
    document.getElementById("bul_pl_logo_text").style.font = "1rem CircularStd";
  } else {
    document.getElementById("bul_pl_universal_header").style.height = "85";
    document.getElementById("bul_pl_universal_header").style.padding = "12px 0px 12px 25px";
    document.getElementById("bul_pl_brown_shield_svg").style.height = "75";
    document.getElementById("bul_pl_header_links").style.margin = "5ex 50px 0 0";
    document.getElementById("bul_pl_logo_text").style.margin = "35px 0px 0px 43px";
    document.getElementById("bul_pl_logo_text").style.font = "1.2rem CircularStd";

  }
}

var modal = document.querySelector(".modal");
var trigger = document.querySelector(".trigger");
var closeButton = document.querySelector(".close-button");

function toggleModal() {
    modal.classList.toggle("show-modal");
    document.getElementById("searchForm_lookfor").focus();
}

function windowOnClick(event) {
    if (event.target === modal) {
        toggleModal();
    }
}

trigger.addEventListener("click", toggleModal);
closeButton.addEventListener("click", toggleModal);
window.addEventListener("click", windowOnClick);

</script>
</header>
</div>
